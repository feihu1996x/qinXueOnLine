INSERT INTO `organizations_teacher` VALUES (2,'艾吉奥·奥迪托雷',40,'刺客组织','刺客集团首领','非常的号召力和人格魅力',5,0,'2018-07-12',5,'resource/images/head_shot/2018/07/23.jpg',20),(3,'马里奥·奥迪托雷',60,'刺客组织','艾吉奥的叔叔，也是艾吉奥进入刺客行列的领路人，导师','表面是功勋卓著的雇佣兵长贵族，真实身份却是刺客头目',2,1,'2018-07-12',5,'resource/images/head_shot/2018/07/22.jpg',20),(4,'克劳迪娅·奥迪托雷',10,'刺客组织','艾吉奥的妹妹，一度过着衣食无忧的大小姐日子','将妓院改造为一个秘密的刺客据点，Claudia最终成为一名刺客',0,0,'2018-07-12',5,'resource/images/head_shot/2018/07/21.jpg',20),(5,'拉·沃尔佩',50,'刺客组织','出场于前作《刺客信条2》，帮助过艾吉奥，于1498年销声匿迹','Vople对Machiavelli极其的不信任，一直怀疑其是内奸',0,0,'2018-07-12',5,'resource/images/head_shot/2018/07/20.jpg',20),(6,'莱昂纳多·达·芬奇',50,'刺客组织','举世瞩目的画家，发明家，科学家','艾齐奥母亲的朋友，也是艾吉奥的朋友，无论在佛罗伦萨、威尼斯还是罗马，艾吉奥都能和达芬奇不期而遇',0,0,'2018-07-14',5,'resource/images/head_shot/2018/07/24.jpg',20),(7,'巴托洛缪·德·阿尔维亚诺',20,'刺客集团','佣兵工会头目','在威尼斯船厂帮助过艾吉奥',0,0,'2018-07-14',5,'resource/images/head_shot/2018/07/25.jpg',20),(8,'罗德里戈·博尔吉亚/亚历山大六世',100,'圣殿骑士','最终BOSS，圣殿骑士头目','害死了艾齐奥的父兄，但最终艾吉奥并未杀死他',0,0,'2018-07-14',4,'resource/images/head_shot/2018/07/26.jpg',200),(9,'恺撒·博尔吉亚',50,'圣殿骑士','亚历山大六世教皇与情妇的私生子','Cesare生性狡诈，居无定所，还一度离开罗马不知去向，给刺客集团的猎杀行动造成了很大的困难',0,0,'2018-07-14',5,'resource/images/head_shot/2018/07/27.jpg',40),(10,'尼可罗·马基亚维利',40,'刺客集团','著名政治哲学家、音乐家、诗人、和浪漫喜剧剧作家','马基亚维利是一个貌奸实忠的人，有着上层社会的身份，却实则阿萨辛刺客集团罗马分支首领',0,0,'2018-07-14',5,'resource/images/head_shot/2018/07/28.jpg',50),(11,'但丁',100,'鬼泣','鬼泣','鬼泣',0,0,'2018-07-16',16,'resource/images/head_shot/2018/07/33.jpg',24);
